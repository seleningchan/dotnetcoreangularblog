{
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Client": {
        "ClientPath": "ClientApp"
    },
    "DbConfiguration": {
        "ConnectionString": "#{ConnectionString}#"
    },
    "SEOConfiguration": {
        /*
            Supported mappings:
            1. id
            2. alias
            3. date/alias
            4. category/alias
            5. date/category/alias
        */
        "ArticleRouteMapping": "date/alias"
    }
}